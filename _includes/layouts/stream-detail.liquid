---
layout: _stream-html
noindex: true
---
{%- render "partials/header", collectionsAll: collections.all, page: page, env: env, back: true -%}
<div id="notesContainer">
  <div class="note" data-note-url="{{ page.url }}">
    <div class="noteWrapper">
      <div class="content markdown-body">
        {{ content }}
      </div>
      <div class="metaInfo">
        <a class="noteURL" href="{{ env.SERVER_ENV | append: page.url }}" target="_blank">Permanent Link</a> Â·
        <time class="updatedAt">{{ page.date | postTime }}</time>
      </div>
    </div>
  </div>
</div>